@model IEnumerable<Empresas.Models.USUARIO>

@{
    ViewBag.Title = "Index";
}



<h2 style="color:#000000" ;>Inicio</h2>

<p style="color:#000000" ;>
    @Html.ActionLink("Crear Nuevo Usuario", "Create")
</p>
<table class="table">
    <tr>
        <th style="color:#000000" ;>
            @Html.DisplayNameFor(model => model.NOMBRE)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr style="color:#000000" ;>
            <td>
                @Html.DisplayFor(modelItem => item.NOMBRE)
            </td>
            <td>
                @Html.ActionLink("Editar", "Edit", new { id = item.NOMBRE }) |
                @Html.ActionLink("Detalles", "Details", new { id = item.NOMBRE }) |
                @Html.ActionLink("Eliminar", "Delete", new { id = item.NOMBRE })
            </td>
        </tr>
    }

</table>
